<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MCPA_Home">

    <ImageView
        android:id="@+id/HomeCarosoul"
        android:layout_width="403dp"
        android:layout_height="127dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="78dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/ClothingLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clothing"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="312dp" />

    <ImageButton
        android:id="@+id/SaleLinkImageButton"
        android:layout_width="414dp"
        android:layout_height="103dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="385dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/RecentPurchaseCarosol"
        android:layout_width="414dp"
        android:layout_height="153dp"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="532dp"
        tools:srcCompat="@tools:sample/avatars" />

    <SearchView
        android:layout_width="388dp"
        android:layout_height="87dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="213dp" />

    <TextView
        android:id="@+id/ShoesLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shoes"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="312dp" />

    <TextView
        android:id="@+id/GroceriesLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Groceries"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="312dp" />

    <TextView
        android:id="@+id/FruitVegLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fruit&Veg"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="312dp" />

    <ImageButton
        android:id="@+id/ProfileLinkButton"
        android:layout_width="47dp"
        android:layout_height="67dp"
        tools:layout_editor_absoluteX="365dp"
        tools:layout_editor_absoluteY="1dp"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>