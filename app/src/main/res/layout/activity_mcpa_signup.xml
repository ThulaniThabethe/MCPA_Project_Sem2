<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".McpaSignup">

    <TextView
        android:id="@+id/RegisterHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="88dp" />

    <TextView
        android:id="@+id/NameRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="128dp" />

    <TextView
        android:id="@+id/SurnameRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Surname"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="180dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CellPhoneNumber"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="245dp" />

    <TextView
        android:id="@+id/EmailRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/PasswordRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="342dp" />

    <TextView
        android:id="@+id/ConfirmPasswordRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ConfirmPassword"
        tools:layout_editor_absoluteX="63dp"
        tools:layout_editor_absoluteY="378dp" />

    <Button
        android:id="@+id/RegisterButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Proceed"
        tools:layout_editor_absoluteX="172dp"
        tools:layout_editor_absoluteY="454dp" />

    <TextView
        android:id="@+id/LoginLink"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"
        tools:layout_editor_absoluteX="237dp"
        tools:layout_editor_absoluteY="554dp" />

    <EditText
        android:id="@+id/NameRegisterInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Name"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="109dp" />

    <EditText
        android:id="@+id/SurnameRegisterInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Surname"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="169dp" />

    <EditText
        android:id="@+id/CellPhoneNumberRegisterInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="CellPhoneNumber"
        tools:layout_editor_absoluteX="175dp"
        tools:layout_editor_absoluteY="219dp" />

    <EditText
        android:id="@+id/EmailRegisterInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Email"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="277dp" />

    <EditText
        android:id="@+id/PasswordRegisterInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Password"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="318dp" />

    <EditText
        android:id="@+id/ConfirmPasswordRegisterInput"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="ConfirmPassword"
        tools:layout_editor_absoluteX="165dp"
        tools:layout_editor_absoluteY="378dp" />

</androidx.constraintlayout.widget.ConstraintLayout>